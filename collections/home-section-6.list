<squarespace:query collection="home-hero-banner">
                       {.repeated section items}
                        {@|apply hero-banner.block}
                        {.end}
                     </squarespace:query>

<section class="hm-testimonial-section center">
                          <div class="container">
                              <div class="row">
                                   {.repeated section items}
                                      <div class="col-md-6 testimonial-boxes">
                                      <div class="taring-stars">
                                          <i class="fas fa-star"></i>
                                          <i class="fas fa-star"></i>
                                          <i class="fas fa-star"></i>
                                          <i class="fas fa-star"></i>
                                          <i class="fas fa-star"></i>
                                      </div>
                                          <h6 class="testimonial-body">{body}</h6>
                                          <h4>{title}</h4>
                                      </div>
                                    {.end}
                              </div>
                          </div>
                      </section>