<section class="blue-banner about-banner inner-page-banner">
        <div class="container">
        <squarespace:query collection="about-banner">
           {.repeated section items}
            <div class="row common-contents">
                <div class="col-md-6 home-banner-left">
                    <h1 class="sec-clr ft-71 fw-bold">{title}</h1>
                    {body}
                    <!--<a href="#"><button>Create Your Free Profile</button></a>-->
                </div>
                <div class="col-md-6 about-banner-right-sec">
                    <img src="../assets/images/about-round.png" alt="abt-banner-right">
                </div>
            </div>
            {.end}
        </squarespace:query>
        </div>

    </section>
    <section class="account-pricing-section">
	<squarespace:query collection="pricing-users" limit="4">
        <div class="container">
            <div class="row">
                <div class="col-md-5">
                    <h3 class="ft-35 fw-bold prm-clr">{collection.title}<br><span class="sec-clr">No Commitment!</span></h3>
                </div>
                <div class="col-md-2">
                </div>
                <div class="col-md-5">
                    <p>Fees only apply to transactions</p>
                    <div class="flex">
                        <a href="#"><button>Request a Demo</button></a>
                        <a href="#"><button>Create Your Profile</button></a>
                    </div>
                </div>
            </div>
            <div class="row pricing-box-row">
                
                    {.repeated section items}
                        <div class="col-md-3  pricing-box-col">
                            <div class="pricing-boxes">
                                <h4 class="fw-bold">{title}</h4>
                                {body}
                            </div>
                        </div>
                    {.end}
                
            </div>
        </div>
	</squarespace:query>
    </section>
